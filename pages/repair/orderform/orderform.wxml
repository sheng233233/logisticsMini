<!--pages/repair/orderform/orderform.wxml-->
<template name="repair-order-temp">
  <!-- 标题图片 -->
  <view>
    <van-image width="320px" height="160px" fit="contain" src="/pages/images/title.jpg" />
  </view>

  <!-- 登录入口 -->
  <view>
    <van-grid column-num="4">
      <van-grid-item icon="photo-o" use-slot bindtap="to_create">
        报修申请
      </van-grid-item>
      <van-grid-item icon="photo-o" use-slot bindtap="to_history">
        历史订单
      </van-grid-item>
    </van-grid>
  </view>





</template>